<script>
    $(document).ready(function () {
        
        let checkauth = window.localStorage.getItem("token");
        let collection = document.getElementById("loggedIn");
        if (checkauth) {
            $("#loggedIn").hide();
            $("#userprofile").show();
        } else {
            $("#loggedIn").show();
            $("#userprofile").hide();
        }
        let welcome = window.localStorage.getItem('welcome');
        let register = window.localStorage.getItem('register');
        let ChangePassword = window.localStorage.getItem('ChangePassword');
        let forgotpassword = window.localStorage.getItem('forgotpassword');
        let forgootPassword = window.localStorage.getItem('forgootPassword');
        let logoout = window.localStorage.getItem('logoout');
        let logooutuser = window.localStorage.getItem('logooutuser');
        let admission = window.localStorage.getItem('admission');
        if (welcome) {
            // alert('Login Successfully, Welcome');
            swal("Welcome To Reality Walk Of USA Website", "Login Successfully", "success");
            window.localStorage.removeItem('welcome');
        }else if (register) {
            // alert('Login Successfully, Welcome');
            swal("Register Successfully" , "Login Please" , "success");
            window.localStorage.removeItem('register');
        }else if (ChangePassword) {
            // alert('Login Successfully, Welcome');
            swal("Password Change Successfully" , "Password is updated" , "success");
            window.localStorage.removeItem('ChangePassword');
        }else if (forgotpassword) {
            // alert('Login Successfully, Welcome');
            swal("Password Reset Email Sent..." , "Please Check Your Email" , "success");
            window.localStorage.removeItem('forgotpassword');
        }else if (forgootPassword) {
            // alert('Login Successfully, Welcome');
            swal("Password Change Successfully" , "Password is updated" , "success");
            window.localStorage.removeItem('forgootPassword');
        }else if (logoout) {
            // alert('Login Successfully, Welcome');
            swal("Logout Successfully" , "GoodBye! Have a nice day." , "success");
            window.localStorage.removeItem('logoout');
        }else if (logooutuser) {
            // alert('Login Successfully, Welcome');
            swal("Logout Successfully" , "GoodBye! Have a nice day." , "success");
            window.localStorage.removeItem('logooutuser');
        }else if (admission) {
            // alert('Login Successfully, Welcome');
            swal("Admission apply Successfully" , "Thankyou! Response in email in few days." , "success");
            window.localStorage.removeItem('admission');
        }

        $('#logout').click(function (e) {
            e.preventDefault();
            $.ajax({
                type: "GET",
                url: "/auth/logoutuser",
                headers: { "authorization": localStorage.getItem('token') },
                success: function (msg) {
                    if (msg.status === 'failed') {
                        $('#error_response').append(msg.message)
                    } else {
                        window.localStorage.removeItem('token');
                        window.localStorage.removeItem('user');
                        window.localStorage.setItem('logoout', 1);
                        window.location.href = "/"
                    }
                }
            })
        });
    });
</script>

<script>
    let userData = window.localStorage.getItem("user");
    userData = JSON.parse(userData);
    if(userData){
        $('#user_name').append(userData.name);
        $('#user_name1').append(userData.name);
        $('#user_email').append(userData.email);
    }

    let pathname = window.location.pathname;
    if(pathname.includes('/admin') && userData.is_admin == 2) {
        if(pathname.includes('/admin/admin-dashboard') || pathname.includes('/admin/')){
            window.location.href = '/admin/admin-admission-list';
        }
    }else if (pathname.includes('/admin')) {
        if(!userData || userData.is_admin == 0) {
            window.location.href = '/';
        }
    }
</script>
